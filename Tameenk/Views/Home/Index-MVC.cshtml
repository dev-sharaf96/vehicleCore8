   //return true if user Id and birthdate are enterd
   function isUserIDAndBirthDateEntered(userIdCtrl)
    {
        if ($(userIdCtrl).val() != '' && getUserMonthBirthDate(userIdCtrl) != '' && getUserYearBirthDate(userIdCtrl) != '')
           return true;

        return false;
    }
    function getUserMonthBirthDate(userIdCtrl) {
        var georgOrHijriDate = $('#georgDate').is(':visible') ? 'georgDate' : 'hijriDate';
        return $(userIdCtrl).closest('.row').find('#' + georgOrHijriDate).find('.userMonthBirthDate').val();

    }
    function getUserYearBirthDate(userIdCtrl) {
        var georgOrHijriDate = $('#georgDate').is(':visible') ? 'georgDate' : 'hijriDate';
        return $(userIdCtrl).closest('.row').find('#' + georgOrHijriDate).find('.userYearBirthDate').val();
    }
