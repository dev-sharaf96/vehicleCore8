            $('#IsOwner').val("true");
        }
        else {
            $('#IsOwner').val("false");
        }
    }

    //used to make any validation before submiting the form (show offer)
    function ValidateAndSubmitShowOffer()
    {
        $('#page-loading').show();
        //get the form
        var form = $('#btnSubmit').closest('form');
        //check if the form is valid and if not valid we dont show the loading image, to do so we remove the submit event from the form
        // we remove submit event bcz any form when submit will show loading page
        //return false so that not submit form
        if (!form.valid())
        {
                $(form).unbind('submit');
                $('#page-loading').hide();
            return;
        }
            $.ajax({
            url: $(form).attr('action'),
                type: 'POST',
                data: $(form).serializeArray(),
                success: function (data, status, xhr) {
                //$('#page-loading').hide();
                //if success redirect to SearchResult in QuotationController with qtRqstExtrnlId string
                if (data.Success) {
                    window.location = '@Url.Action("SearchResult", "Quotation")' + '?qtRqstExtrnlId=' + data.qtRqstExtrnlId;
                }
                else {
                    $('.BDC_ReloadLink').get(0).click();
                        $(form).validate().showErrors(data.notValidFields);
                    if (data.requestMessage != null) {
                        if (data.requestMessage.Errors != null) {
                            showQuotationErrors(data.requestMessage.Errors);
                        }
                    }
                        $('#page-loading').hide();
                }


            },
                error: function (xhr, status, err) {
                if (xhr.status == 400) {
                    // this is our erroneous result
                }
                else {
                }
            }
        });

    }
    //make a get request to server with list of errors
