@using (Html.BeginForm("Index", "Offers", null, FormMethod.Post, new { onsubmit = "return validate()" }))
{
<div class="container">
    <!-- <div class="row serverErrValidation" [hidden]="!showErrDesc">
          <div class="col-lg-12">
              {{servererror}}
          </div>
      </div> -->
    <div class="terq-bg">
        <div class="row">
            <div class="col-lg-12 col-xs-12">
                <div class="content-wrapper">
                    <div class="flowers-bg"><img src="/resources/imgs/woreef/flowersBg.png"></div>
                    <div class="white-wrapper">
                        <div class="image">
                            <div class="img-parent">
                                <img src="/resources/imgs/woreef/bluebg-title.png">
                                <div class="title">Wareef</div>
                            </div>
                        </div>
                        <div class="row form">
                            <div class="col-lg-6 col-xs-12">
                                <div class="row ">
                                    <div class="col-lg-12 col-xs-12 mb-15 custom-float-left">
                                        <label> @Html.Raw(Tameenk.LangText.NIN)</label>
                                        <input class="customInputsStyle form-control" placeholder="@Html.Raw(Tameenk.LangText.NIN)" type="text" id="DriverNIN" name="DriverNIN" value="@Model.DriverNIN" />
                                        <div id="NINvalidationMessgae" style="text-align: right; color: red" class="col-lg-12"></div>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    @if (Model.ActivePolicyData == null)
                                    {
                                    if (!string.IsNullOrEmpty(Model.ErrorMessage))
                                    {
                                    <div class="errClsMsg">@Model.ErrorMessage</div>
                                    }
                                    }
                                    else if (Model.ActivePolicyData.HasActivePolicy == 0)
                                    {
                                    @*<div class="errClsMsg">@(Model.Language.ToLower() == "en" ? "Doesn't Deserve Discount" : "لا يستحق الخصم")</div>*@
                                    <div class="errClsMsg">
                                        @(lang.Equals(LanguageTwoLetterIsoCode.En.ToString(), StringComparison.InvariantCultureIgnoreCase) ? "Doesn't Deserve Discount" : "لا يستحق الخصم")
                                        <i class="fas fa-times"></i>
                                    </div>
                                    }
                                    else if (Model.ActivePolicyData.HasActivePolicy == 1)
                                    {
                                    @*<div class="succssClsMsg">@(Model.Language.ToLower() == "en" ? $"Deserves Discount and membership expires on {Model.ActivePolicyData.ExpiryDate}" : $"يستحق الخصم وعضويته تنتهى فى {Model.ActivePolicyData.ExpiryDate}")</div>*@
                                    <div class="succssClsMsg">
                                        @(lang.Equals(LanguageTwoLetterIsoCode.En.ToString(), StringComparison.InvariantCultureIgnoreCase) ? $"Deserves Discount and membership expires on {Model.ActivePolicyData.ExpiryDate.Value.ToString("dd-MM-yyyy", new CultureInfo("en-US"))}" : $"يستحق الخصم وعضويته تنتهى فى {Model.ActivePolicyData.ExpiryDate.Value.ToString("dd-MM-yyyy", new CultureInfo("en-US"))}")
                                        <i class="fas fa-check"></i>
                                    </div>
                                    }
                                </div>

                                <div class="row">
                                    @if (Model.Captcha != null)
                                    {
                                    <input type="hidden" name="Captcha.Token" value="@Model.Captcha.Token" />
                                    <input type="hidden" name="Captcha.ExpiredInSeconds" value="@Model.Captcha.ExpiredInSeconds" />
                                    <input type="hidden" name="Captcha.ExpiryDate" value="@Model.Captcha.ExpiryDate" />
                                    <div class="col-lg-9 col-xs-12 custom-float-left">
                                        <div class="caption">
                                            <div class="row">
                                                <div class="col-lg-6 col-xs-12">

                                                    <input class="input-element form-control mt-2" appFloatedLabel type="text" id="captchaCode" name="Captcha.Input" appNumberOnly autocomplete="off" maxlength="4" />
                                                </div>
                                                <div class="col-lg-6 col-xs-12">
                                                    <img class="captcha-img" alt="الرمز" title="الرمز" src="@Model.Captcha.Image">
                                                    <label class="input-label" for="captchaCode"></label>
                                                    <button type="submit" class="reload-btn reloadClsCustom" id="btnRefresh" name="ButtonType" value="Refresh"><i class="fas fa-sync"></i></button>
                                                </div>


                                            </div>

                                        </div>
                                    </div>
                                    }
                                    <div class="col-lg-3 col-xs-12 custom-float-right">
                                        <button id="btnSubmit" name="ButtonType" value="Save" class="mybtnCustomStyle bx-left btn sm pointer dis-block no-trans bx-left txt-white continue txt-brand bg-brand-hvr txt-xs bg-gr-main  circ-rdus l-shadow md-mar">@Html.Raw(Tameenk.LangText.Search) </button>

                                        <div id="validationMessgae" style="text-align: right; color: red" class="col-lg-12"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 col-xs-12">
                <div class="content-wrapper second-header">
                    <!-- <div class="oneflower-bg"><img src="/resources/imgs/woreef/oneflower.png"></div> -->
                    <div class="white-wrapper">

                        <div class="row custom-flex" id="pdfTable" #pdfTable>
                            <div class="col-lg-4 ustom-float-left">
                                <div class="title-img">
                                    <div class="parent-img">
                                        <img src="/resources/imgs/woreef/bluebg-title.png">
                                        <div class="title">Wareef</div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 custom-float-left">
