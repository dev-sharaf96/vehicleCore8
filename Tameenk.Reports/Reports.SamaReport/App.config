  </startup>
  <appSettings>
    <add key="RunServiceEvery" value="" />
    <add key="RunAt" value="00" />
    <add key="RunOnStart" value="False" />
    <add key="MailTo" value="safaa.e@neom-tech.net;mahmoud.g@neom-tech.net" />
    <add key="MailFrom" value="noreply@bcare.com.sa" />
    <add key="Testing_Environment" value="true" />
